package entities;

import java.util.ArrayList;

public class Voter {

	private ArrayList<VoterType> listVoter = new ArrayList<VoterType>();
	
	public Voter() {
		for(int i = 0; i < 800; i++) {
			listVoter.add(VoterType.valid);
		}
		for(int i = 0; i < 150; i++) {	
			listVoter.add(VoterType.white);
		}
		for(int i = 0; i < 50; i++) {
			listVoter.add(VoterType.nullo);
		}		
	}	
} 